{
  "name": "common_services",
  "version": "1.0.0",
  "description": "",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "generate:proto3": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=lib/generated lib/proto/service.proto",
    "generate:proto2": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts.cmd --ts_out=grpc_js:. -I lib/proto lib/proto/*.proto",
    "generate:proto-server": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_out=./lib/generated --ts_proto_opt=fileSuffix=.server --ts_proto_opt=outputServices=grpc-js --ts_proto_opt=esModuleInterlop=true -I=lib/proto/ lib/proto/*.proto",
    "generate:proto-client": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_out=./lib/generated --ts_proto_opt=fileSuffix=.client --ts_proto_opt=outputClientImpl=grpc-web --ts_proto_opt=esModuleInterlop=true -I=lib/proto/ lib/proto/*.proto",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.4",
    "@grpc/proto-loader": "^0.7.12",
    "@improbable-eng/grpc-web": "^0.15.0",
    "amqplib": "^0.10.3",
    "grpc-tools": "^1.12.4",
    "grpc-web": "^1.5.0",
    "protoc": "^1.1.3",
    "rimraf": "^5.0.5",
    "ts-proto": "^1.170.0",
    "ts-protoc-gen": "^0.15.0",
    "winston": "^3.13.0"
  }
}
